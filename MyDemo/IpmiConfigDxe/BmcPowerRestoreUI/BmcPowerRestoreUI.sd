#define BMC_POWER_RESTORE_POLICY_ONEOF\
    oneof varid = BmcIpmiConfigVar.PowerRes,\
        questionid = BMC_POWER_RESTORE_POLICY_KEY,\
        prompt      = STRING_TOKEN(STR_POWER_RESTORE_OPTION),\
        help        = STRING_TOKEN(STR_POWER_RESTORE_OPTION_HELP),\
        option text = STRING_TOKEN(STR_POWER_OFF), value = 0x0, flags = INTERACTIVE;\
        option text = STRING_TOKEN(STR_POWER_HOLD),  value = 0x1, flags = INTERACTIVE;\
        option text = STRING_TOKEN(STR_POWER_ON), value = 0x2, flags = DEFAULT | INTERACTIVE;\
        option text = STRING_TOKEN(STR_UNKNOWN), value = 0xf, flags = INTERACTIVE;\
    endoneof;

